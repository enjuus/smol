#!/usr/bin/env fish

set font '-*-lemon-medium-r-*-*-*-*-*-*-m-*-iso10646-1'
set px 25
set ph 25
set pw 25
set py 50

set icon тон
set icon2 тоХ

feh --bg-fill '/home/fullsalvo/Pictures/Walls/WindowRain.jpg'

echo "%{A:$HOME/.smol/popups/msc/launcher &:}  $icon2  %{A}" | lemonbar -b -p -f "$font" -g $pw"x$ph+$px+$py" -B '#eeeeee' -F '#aa8574' | sh
